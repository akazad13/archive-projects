@model IEnumerable<WMC.Models.User>

<div class="row g-3 mb-4 align-items-center justify-content-between">
    <div class="col-9">
        <h1 class="app-page-title mb-0">View Feedbacks</h1>
    </div>
    <div class="col-2">
        <button type="button" class="btn btn-primary float-right" id="addNewStaff" style="color: #fff">Add New Staff</button>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <table class="table data-table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Staff's Full Name</th>
                    <th>Login ID</th>
                    <th>Contact Number</th>
                </tr>
            </thead>
            <tbody class="grid-table-body">
                @{
                    int i = 0;
                }
                @foreach (var item in Model)
                {
                    i += 1;
                    <tr>
                        <td>@(i)</td>
                        <td>@Html.DisplayFor(model => item.Name)</td>
                        <td>@Html.DisplayFor(model => item.UserName)</td>
                        <td>@Html.DisplayFor(model => item.PhoneNumber)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
