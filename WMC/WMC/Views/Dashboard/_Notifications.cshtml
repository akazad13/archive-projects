@model IEnumerable<string>

<div class="row g-3 mb-4 align-items-center justify-content-between">
    <div class="col-9">
        <h1 class="app-page-title mb-0">Notifications</h1>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @foreach (var notificaiton in Model)
        {
            <div class="card">
                <div class="card-body">
                    @notificaiton
                </div>
            </div>
        }

    </div>
</div>